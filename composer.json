{
    "name": "beastbytes/token",
    "description": "Token management library to generate and validate tokens",
    "keywords": [
        "token",
        "UUID",
        "yii3"
    ],
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "BeastBytes\\Token\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeastBytes\\Token\\Tests\\": "tests/"
        }
    }
}